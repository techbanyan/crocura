-if !current_user
	.container-fluid
		.row-fluid.login-box-container
			.span12
				%ul(class="login-box-text")
					%li To Like and Comment
					-session[:return_to] = "#{request.protocol + request.host}:#{request.port.to_s + request.fullpath}"
					%li=link_to image_tag("sign-in-with-instagram.png"), "/auth/instagram"