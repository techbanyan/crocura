.whitespace10
.container-fluid
	.row-fluid
		.span12
			%ul.explore-pictures-tabs-span
				%li
					=link_to "Popular", root_url, :class => "explore-pictures-tabs", :remote => true
				-@tabs.each do |tab|
					%li
						=link_to tab.query_name, root_url(:querytype => tab.query_type, :query => tab.query, :latitude => tab.latitude.to_s, :longitude => tab.longitude.to_s), :class => "explore-pictures-tabs", :remote => true
					